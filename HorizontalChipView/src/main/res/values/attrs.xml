<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalChipView">
        <attr name="title" format="string" />
        <attr name="titleColor" format="color" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="titleTypeface" format="string" />
        <attr name="requireSelection" format="boolean"/>
        <attr name="singleCheck" format="boolean"/>
        <attr name="selectedChipId" format="reference"/>
        <attr name="chipLayout" format="reference"/>
    </declare-styleable>
</resources>